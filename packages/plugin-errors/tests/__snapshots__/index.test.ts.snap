  issues: [InputValidationIssue!]
  message: String
}

type InputValidationIssue {
  message: String
  path: [String!]
}

type LengthError implements Error {
  message: String
  minLength: Int
}

type Query {
  asyncItemErrors(error: Boolean): QueryAsyncItemErrorsResult
  directResult(shouldThrow: Boolean): QueryDirectResultResult
